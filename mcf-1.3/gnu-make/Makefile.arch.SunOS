CC = gcc
GCC = gcc

CFLAGS  = -Wall -Winline

ifeq ($(WARN),error)
	 CFLAGS += -Werror
endif

CFLAGS += -g

ifeq ($(LEVEL),opt)
	 CFLAGS += -O6
endif

CFLAGS += -mcpu=ultrasparc -funroll-all-loops -funroll-loops
